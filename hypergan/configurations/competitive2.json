
{
  "description": "Competitive optimizer with Adam",
  "discriminator": 
  {
    "class": "class:hypergan.discriminators.configurable_discriminator.ConfigurableDiscriminator",
    "defaults":{
      "filter": [3,3],
      "stride": [1,1],
      "avg_pool": [2,2],
      "activation": "lrelu",
      "initializer": "he_normal",
      "lrmul": 1.0,
      "random_stddev": 0.02
    },
    "layers":[
      "conv 64 ", 
      "conv 128",
      "conv 256",
      "conv 512",
      "conv 512",
      "linear 1 activation=null initializer=he_normal bias=false"
    ]
  },

  "latent":
  {
    "class": "function:hypergan.distributions.uniform_distribution.UniformDistribution",
    "min": -1,
    "max": 1,
    "z": 100
  },
  "generator": {
    "class": "function:hypergan.generators.resizable_generator.ResizableGenerator",
    "defaults":{
      "initializer": "he_normal",
      "lrmul": 1.0,
      "activation": "lrelu"
    },
    "final_activation": "clamped_unit",
    "final_depth": 64,
    "depth_increase": 64,
    "adaptive_instance_norm": "w",
    "adaptive_instance_norm_layers": 2,
    "adaptive_instance_norm_last_layer": true,
    "block": "deconv"
  },
 "loss": {
    "class": "function:hypergan.losses.logistic_loss.LogisticLoss",
    "reduce": "reduce_mean"
  },

  "trainer": {
    "class": "function:hypergan.trainers.simultaneous_trainer.SimultaneousTrainer",
    "optimizer": {
      "class": "function:hypergan.optimizers.experimental.competitive2_optimizer.CompetitiveOptimizer",
      "conjugate6": true,
      "neg": true,
      "sum": "=",
      "nsteps": 12,
      "sga_lambda": 0,
      "learn_rate": 1e-4,
      "optimizer": {
        "class": "function:tensorflow.python.training.adam.AdamOptimizer",
        "learn_rate": 1e-4,
        "beta1":0.0,
        "beta2": 0.9973000000000001
      }
    }
  },
  "runtime": {
    "anysize": true,
    "train": "hypergan train [dataset] --sampler static_batch -b 8 --size 64x64x3"
  },
  "hypergan_version": "0.10.0",
  "class": "class:hypergan.gans.standard_gan.StandardGAN"
}
